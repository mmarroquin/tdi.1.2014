<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <center>
<%= image_tag @graph %>
<br><hr>
<%= image_tag @graph2 %>
<br><hr>
<% if @graph7 ==  "La BD de Reportes esta vacia"%>
  La BD de Reportes está vacía
<% else %>
  <%= image_tag @graph7 %>
<% end %>
<br><hr>
<% if @graph6 ==  "La BD de productos o la BD de WebProductos esta vacia"%>
  La BD de productos o la BD de WebProductos está vacía
<% else %>
  <%= image_tag @graph6 %>
<% end %>

</center>
  <tbody>
    <% @dashboards.each do |dashboard| %>
      <tr>
        <td><%= link_to 'Show', dashboard %></td>
        <td><%= link_to 'Edit', edit_dashboard_path(dashboard) %></td>
        <td><%= link_to 'Destroy', dashboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

